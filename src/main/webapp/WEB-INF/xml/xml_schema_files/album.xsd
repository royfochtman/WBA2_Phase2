<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.example.org/photoBay" elementFormDefault="qualified"
	xmlns:alb="http://www.example.org/photoBay">
	<xs:complexType name="albumType">
		<xs:all maxOccurs="1" minOccurs="0">
			<xs:element name="title" type="xs:string" minOccurs="1"></xs:element>
			<xs:element name="description" type="xs:string" minOccurs="1"></xs:element>
			<xs:element name="photos" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="photo" type="xs:base64Binary" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
		<xs:attribute name="ID" type="xs:ID" use="required"></xs:attribute>
		<xs:attribute name="dateOfCreation" type="xs:date" use="required"></xs:attribute>
	</xs:complexType>
</xs:schema>
