<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/photoBay"  elementFormDefault="qualified" xmlns:login="http://www.example.org/photoBay">
<xs:element name="login">       
        <xs:complexType>
			<xs:all maxOccurs="1" minOccurs="1">
				<xs:element name="email" type="xs:string"></xs:element>
				<xs:element name="password" type="xs:string"></xs:element>
			</xs:all> 
	</xs:complexType>
    </xs:element>
</xs:schema>
