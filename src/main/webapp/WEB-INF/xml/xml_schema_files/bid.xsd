<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/photoBay"  elementFormDefault="qualified" xmlns:bid="http://www.example.org/photoBay">
<xs:element name="bid">       
        <xs:complexType>
			<xs:all maxOccurs="1" minOccurs="1">
				<xs:element name="bidValue" type="xs:positiveInteger"></xs:element>
			</xs:all> 
			<xs:attribute name="ID" type="xs:int" use="required"/>
			<xs:attribute name="ref" type="xs:anyURI"/>
			<xs:attribute name="photoSellRef" type="xs:anyURI" use="required"/>
			<xs:attribute name="pressAgencyRef" type="xs:anyURI" use="required"></xs:attribute>
	</xs:complexType>
    </xs:element>
</xs:schema>
