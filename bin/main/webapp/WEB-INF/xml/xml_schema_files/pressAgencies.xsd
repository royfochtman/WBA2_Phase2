<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/photoBay"  elementFormDefault="qualified" xmlns:pres="http://www.example.org/photoBay">
<xs:element name="pressAgencies">       
        <xs:complexType>
			<xs:sequence minOccurs="0" maxOccurs="unbounded" >
				<xs:element name="pressAgencyRef">
					<xs:complexType>
						<xs:all minOccurs="1">
							<xs:element name="name" type="xs:string"/>
							<xs:element name="uri" type="xs:anyURI"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence> 
		</xs:complexType>
    </xs:element>
</xs:schema>
