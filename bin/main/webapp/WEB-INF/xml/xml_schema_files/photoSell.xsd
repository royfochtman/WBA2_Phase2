<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/photoBay"  elementFormDefault="qualified" xmlns:phs="http://www.example.org/photoBay">
<xs:include schemaLocation="topic.xsd"></xs:include>
<xs:element name="photoSell">       
        <xs:complexType>
			<xs:all maxOccurs="1" minOccurs="1">
				<xs:element name="price" type="xs:positiveInteger"></xs:element>
				<xs:element name="description" type="xs:string"></xs:element>
				<xs:element name="photo" type="xs:base64Binary"></xs:element>
				<xs:element name="topics" type="phs:topicType"></xs:element>
				<xs:element name="status">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="new"></xs:enumeration>
						<xs:enumeration value="closed"></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>	
			</xs:element>
			</xs:all> 
			
			<xs:attribute name="ID" type="xs:int"></xs:attribute>
			<xs:attribute name="photographerID" type="xs:int" use="required"></xs:attribute>
	</xs:complexType>
    </xs:element>
</xs:schema>
